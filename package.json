{
  "name": "getreceipts-web",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "lint:strict": "eslint --max-warnings 0",
    "lint:fix": "eslint --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "pre-commit": "npm run type-check && npm run lint:strict && npm run test",
    "ci": "npm run type-check && npm run lint:strict && npm run test && npm run build",
    "pre-push": "npm run ci",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:seed": "ts-node scripts/seed.ts",
    "version:increment": "node scripts/increment-version.mjs",
    "push": "npm run version:increment && git add version.toml && git commit -m \"Auto-increment version [skip ci]\" && git push",
    "prepare": "husky"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@react-sigma/core": "^5.0.4",
    "@supabase/supabase-js": "^2.56.0",
    "@types/d3": "^7.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "d3": "^7.9.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "drizzle-orm": "^0.44.4",
    "graphology": "^0.26.0",
    "lru-cache": "^11.1.0",
    "lucide-react": "^0.540.0",
    "nanoid": "^5.1.5",
    "next": "15.5.0",
    "pg": "^8.16.3",
    "pgvector": "^0.2.1",
    "postgres": "^3.4.7",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-force-graph-2d": "^1.28.0",
    "react-force-graph-3d": "^1.28.0",
    "react-resizable-panels": "^3.0.5",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5",
    "vitest": "^3.2.4"
  }
}
